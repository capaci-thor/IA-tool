#:kivy 1.9.2
<ManhattanBox>:
    orientation:"vertical"

    BoxLayout:
        orientation:"vertical"
        size_hint: 1, 0.16
        pos_hint:{"left":1,"top":1}

        Button:
            text:"Cargar"
            on_press: app.root.cargar()
            background_color: utils.get_color_from_hex("#FF803D")
            background_normal: ''
        Label:
            id:carga
            text: root.cargaTexto
            color: 1,0,0,1
        Button:
            text:"Aplicar"
            on_press: app.root.aplicarManhattan()
            background_color: utils.get_color_from_hex("#FF803D")
            background_normal: ''

    BoxLayout:
        orientation:"vertical"
        size_hint: 1, 0.9